#include <stdio.h>
#include <unistd.h>


int main() {
    int matrice[10][20];


    for (int i = 0; i < 10; i++) {
        for (int j = 0; j < 20; j++) {
            matrice[i][j] = 0;
        }
    }
    for (int i = 0; i < 10; i++) {
        for (int j = 0; j < 20; j++) {
            printf("%d ", matrice[i][j]);
        }
        printf("\n");
    }


printf("\n");
    printf("Vous avez 120 secondes : \n");
    int seconds = 120;

    while (seconds >= 0) {
        printf("%d ", seconds);
        fflush(stdout);
        sleep(1);
        seconds--;

    }

    printf("\n");
return 0;
}
